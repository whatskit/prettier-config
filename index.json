{
  "$schema": "http://json.schemastore.org/prettierrc",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "cursorOffset": -1,
  "insertPragma": false,
  "printWidth": 120,
  "proseWrap": "preserve",
  "requirePragma": true,
  "jsxBracketSameLine": true, 
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": true
}