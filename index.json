{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "tabWidth": 2, 
  "useTabs": false, 
  "semi": false, 
  "useTabs": true, 
  "singleQuote": false,
  "trailingComma": "es5", 
  "bracketSpacing": true, 
  "jsxBracketSameLine": true, 
  "arrowParens": "always", 
  "htmlWhitespaceSensitivity": "ignore", 
  "quoteProps": "as-needed",
  "requirePragma": true,
  "proseWrap": "preserve"
}
